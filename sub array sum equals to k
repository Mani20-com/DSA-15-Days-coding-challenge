from collections import defaultdict 
class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
        count=0
        current_sum=0
        prefix_sum=defaultdict(int)
        prefix_sum[0]=1
        for i in nums:
            current_sum+=i
            if(current_sum-k) in prefix_sum:
                count+=prefix_sum[current_sum-k]
            prefix_sum[current_sum]+=1
        return count                



        
